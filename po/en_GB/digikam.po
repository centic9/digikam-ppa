msgctxt "@option:radio"
msgid "Rotate by only setting a flag"
msgstr "Rotate by only setting a flag"

#: utilities/setup/setupmetadata.cpp:352
msgctxt "@option:radio"
msgid "Rotate by changing the content if possible"
msgstr "Rotate by changing the content if possible"

#: utilities/setup/setupmetadata.cpp:353
