msgid "Luminosity"
msgstr "Luminosité"

#: libs/dimg/filters/levels/histogrambox.cpp:275
#, kde-format
msgid "<b>Luminosity</b>: display the image's luminosity values."
msgstr "<b>Luminosité</b> : affiche les valeurs de luminosité de l'image."

#: libs/dimg/filters/levels/histogrambox.cpp:277
#, kde-format
msgctxt "The red channel"
msgid "Red"
msgstr "Rouge"

#: libs/dimg/filters/levels/histogrambox.cpp:278
#, kde-format
msgid "<b>Red</b>: display the red image-channel values."
msgstr "<b>Rouge</b> : affiche les valeurs du canal rouge."

#: libs/dimg/filters/levels/histogrambox.cpp:280
#, kde-format
msgctxt "The green channel"
msgid "Green"
msgstr "Vert"

#: libs/dimg/filters/levels/histogrambox.cpp:281
#, kde-format
msgid "<b>Green</b>: display the green image-channel values."
msgstr "<b>Vert</b> : affiche les valeurs du canal vert."

#: libs/dimg/filters/levels/histogrambox.cpp:283
#, kde-format
